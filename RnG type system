import random

def card_pulled(nums):

    if 5 >= nums:
        return (nums, "LEGGO")
    elif 15 >= nums:
        return (nums, "EPIC")
    elif 30 >= nums:
        return (nums, "RARE")
    elif 55 >= nums:
        return (nums, "UNCOMMON")
    elif 100 >= nums:
        return (nums, "COMMON")

i = 1
while i < 4:
    print(card_pulled(random.randint(1, 100)))
    i += 1
